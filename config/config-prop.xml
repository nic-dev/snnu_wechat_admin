	<entry key="database.driverName">com.mysql.jdbc.Driver</entry>
	<entry key="database.url">jdbc:mysql://localhost:3306/snnu_wechat?useUnicode=true&amp;characterEncoding=UTF-8</entry>
	<entry key="database.user">root</entry>
	<entry key="database.password">vv</entry>
	<entry key="database.initialSize">0</entry>
